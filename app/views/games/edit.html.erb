<% provide(:title, 'Edit game') %>

<h1>Edit game</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@game) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= render 'gamefields', f: f %>
      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
